<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE api SYSTEM "api.dtd">

<api name="CSF">

  <constant name="CSF"            val="csf"/>
  <constant name="LOCATION"       val="location"/>
  <constant name="PACKAGE"        val="package"/>
  <constant name="INFO"           val="info"/>
  <constant name="CLASS"          val="class"/>
  <constant name="METHOD"         val="method"/>

  <constant name="WHERE"          val="where"/>
  <constant name="RETVAL"         val="retval"/>
  <constant name="ARG"            val="arg"/>
  <constant name="VARIABLE"       val="variable"/>
  <constant name="ENUM"           val="enum"/>
  <constant name="ENUMVAL"        val="enumval"/>
  <constant name="TYPESPEC"       val="typespec"/>
  <constant name="ACCESS"         val="access"/>
  <constant name="INHERIT"        val="inherit"/>
  <constant name="COMMENT"        val="comment"/>
  <constant name="DIRECTIVE"      val="directive"/>
  <constant name="TEXT"           val="text"/>

  <class name="Toplevel" elmname="csf">
    <var type="string"  name="language"/>
    <var type="ptrlist" name="content"/>

    <attr    type="string"   name="language" var="language"/>
    
    <subelem type="ptrlist" name="package"   var="content"/>
    <subelem type="ptrlist" name="method"    var="content"/>
    <subelem type="ptrlist" name="class"     var="content"/>
    <subelem type="ptrlist" name="enum"      var="content"/>
    <subelem type="ptrlist" name="variable"  var="content"/>
    <subelem type="ptrlist" name="typespec"  var="content"/>
    <subelem type="ptrlist" name="comment"   var="content"/>
    <subelem type="ptrlist" name="directive" var="content"/>
  </class>

  <class name="Package" elmname="package">
    <var type="string"  name="id"/>
    <var type="string"  name="name"/>
    <var type="ptr"     name="location"/>
    <var type="ptrlist" name="info"/>
    <var type="ptrlist" name="content"/>

    <attr    type="string"   name="id" var="id"/>
    <attr    type="string"   name="name" var="name"/>

    <subelem type="ptr"      name="location"  var="location"/>
    <subelem type="ptrlist"  name="info"      var="info"/>
    
    <subelem type="ptrlist" name="package"   var="content"/>
    <subelem type="ptrlist" name="method"    var="content"/>
    <subelem type="ptrlist" name="class"     var="content"/>
    <subelem type="ptrlist" name="enum"      var="content"/>
    <subelem type="ptrlist" name="variable"  var="content"/>
    <subelem type="ptrlist" name="typespec"  var="content"/>
    <subelem type="ptrlist" name="comment"   var="content"/>
    <subelem type="ptrlist" name="directive" var="content"/>
    
  </class>

  <class name="Class" elmname="class">
    <var type="string"   name="id"/>
    <var type="string"   name="name"/>
    <var type="ptr"      name="location"/>
    <var type="ptr"      name="access"/>
    <var type="ptrlist"  name="parents"/>
    <var type="ptrlist"  name="info"/>
    <var type="ptrlist"  name="content"/>

    <attr    type="string"   name="id" var="id"/>
    <attr    type="string"   name="name" var="name"/>

    <subelem type="ptr"      name="location"  var="location"/>
    <subelem type="ptr"      name="access"    var="access"/>
    <subelem type="ptrlist"  name="inherit"   var="parents"/>
    <subelem type="ptrlist"  name="info"      var="info"/>
    
    <subelem type="ptrlist" name="package"   var="content"/>
    <subelem type="ptrlist" name="method"    var="content"/>
    <subelem type="ptrlist" name="class"     var="content"/>
    <subelem type="ptrlist" name="enum"      var="content"/>
    <subelem type="ptrlist" name="variable"  var="content"/>
    <subelem type="ptrlist" name="typespec"  var="content"/>
    <subelem type="ptrlist" name="comment"   var="content"/>
    <subelem type="ptrlist" name="directive" var="content"/>
    
  </class>


  <class name="Method" elmname="method">
    <var type="string"  name="id"/>
    <var type="string"  name="name"/>
    <var type="ptrlist" name="where"/>
    <var type="ptr"     name="access"/>
    <var type="ptrlist" name="info"/>
    <var type="ptrlist" name="retvals"/>
    <var type="ptrlist" name="args"/>
    <var type="ptrlist"  name="content"/>


    <attr    type="string"   name="id" var="id"/>
    <attr    type="string"   name="name" var="name"/>

    <subelem type="ptrlist"  name="where"     var="where"/>
    <subelem type="ptr"      name="access"    var="access"/>
    <subelem type="ptrlist"  name="info"      var="info"/>
    <subelem type="ptrlist"  name="retval"    var="retvals"/>
    <subelem type="ptrlist"  name="arg"       var="args"/>

    <subelem type="ptrlist" name="package"   var="content"/>
    <subelem type="ptrlist" name="method"    var="content"/>
    <subelem type="ptrlist" name="class"     var="content"/>
    <subelem type="ptrlist" name="enum"      var="content"/>
    <subelem type="ptrlist" name="variable"  var="content"/>
    <subelem type="ptrlist" name="typespec"  var="content"/>
    <subelem type="ptrlist" name="comment"   var="content"/>
    <subelem type="ptrlist" name="directive" var="content"/>
   </class>  

  <class name="Variable" elmname="variable">
    <var type="string"  name="id"/>
    <var type="string"  name="name"/>
    <var type="ptr"     name="location"/>
    <var type="ptr"     name="access"/>
    <var type="ptrlist" name="info"/>

    <attr    type="string"   name="id" var="id"/>
    <attr    type="string"   name="name" var="name"/>
    
    <subelem type="ptr"      name="location"  var="location"/>
    <subelem type="ptr"      name="access"    var="access"/>
    <subelem type="ptrlist"  name="info"      var="info"/>
  </class>  

  <class name="Enum" elmname="enum">
    <var type="string"  name="id"/>
    <var type="string"  name="name"/>
    <var type="ptr"     name="location"/>
    <var type="ptr"     name="access"/>
    <var type="ptrlist" name="values"/>

    <attr    type="string"   name="id" var="id"/>
    <attr    type="string"   name="name" var="name"/>
    
    <subelem type="ptr"      name="location"  var="location"/>
    <subelem type="ptr"      name="access"    var="access"/>
    <subelem type="ptrlist"  name="enumval"   var="values"/>
  </class>
  
  <class name="Enumval" elmname="enumval">
    <var type="string" name="name"/>
    <var type="string" name="value"/>
    <attr type="string"  name="name"  var="name"/>
    <attr type="string"  name="value" var="value"/>
  </class>
  
  <class name="TypeSpec" elmname="typespec">
    <var type="string"  name="id"/>
    <var type="string"  name="name"/>
    <var type="ptr"     name="location"/>
    <var type="ptr"     name="access"/>
    <var type="ptrlist" name="info"/>

    <attr    type="string"   name="id"      var="id"/>
    <attr    type="string"   name="name"    var="name"/>
    
    <subelem type="ptr"      name="location"  var="location"/>
    <subelem type="ptr"      name="access"    var="access"/>
    <subelem type="ptrlist"  name="info"      var="info"/>
  </class>

  <class name="Comment" elmname="comment">
    <var type="ptr"     name="location"/>
    <var type="string"  name="text"/>

    <subelem type="ptr"      name="location"  var="location"/>
    <subelem type="string"   name="text"      var="text"/>
  </class>
  
  <class name="Directive" elmname="directive">
    <var type="string"  name="name"/>
    <var type="string"  name="value"/>
    <var type="string"  name="info"/>
    <var type="ptr"     name="location"/>

    <attr    type="string"   name="name" var="name"/>
    <attr    type="string"   name="info" var="info"/>
    <attr    type="string"   name="value" var="value"/>
    
    <subelem type="ptr"      name="location"  var="location"/>
  </class>
    
  <class name="Retval" elmname="retval">
    <var     type="ptrlist" name="info"/>
    <subelem type="ptrlist" name="info" var="info"/>
  </class>

  <class name="Arg" elmname="arg">
    <var     type="ptrlist" name="info"/>
    <subelem type="ptrlist" name="info" var="info"/>
  </class>
    
  <class name="Info" elmname="info">
    <var type="string" name="type"/>
    <var type="string" name="value"/>
    <var type="string" name="info"/>
    <attr type="string"  name="type"  var="type"/>
    <attr type="string"  name="value" var="value"/>
    <attr type="string"  name="info"  var="info"/>
  </class>

  <class name="Access" elmname="access">
    <var type="string" name="visibility"/>
    <var type="string" name="scope"/>
    <attr type="string"  name="visibility"  var="visibility"/>
    <attr type="string"  name="scope"       var="scope"/>
  </class>

  <!-- name can also be an id -->
  <class name="Inherit" elmname="inherit">
    <var type="string"  name="name"/>
    <var type="ptrlist" name="info"/>
    <attr    type="string"  name="name" var="name"/>
    <subelem type="ptrlist" name="info" var="info"/>
  </class>

  <class name="Where" elmname="where">
    <var type="string"  name="what"/>
    <var type="ptr"     name="location"/>

    <attr    type="string"   name="what" var="what"/>
    <subelem type="ptr"      name="location"  var="location"/>
  </class>
    
  
  <class name="Location" elmname="location">
    <var type="string" name="file"/>
    <var type="string" name="startline"/>
    <var type="string" name="startcol"/>
    <var type="string" name="endline"/>
    <var type="string" name="endcol"/>
    <var type="string" name="position"/>
    <attr type="string"  name="file"      var="file"/>
    <attr type="string"  name="startline" var="startline"/>
    <attr type="string"  name="startcol"  var="startcol"/>
    <attr type="string"  name="endline"   var="endline"/>
    <attr type="string"  name="endcol"    var="endcol"/>
    <attr type="string"  name="position"  var="position"/>
  </class>

  
</api>
